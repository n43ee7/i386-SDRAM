module Intel80386DX (
	input CLK2,				// CLK
	inout [31:2] D,		// DATA 31-0
	output [31:2] A,		// ADDR 31-2
	output [2:0] BE,		// BE 3-0
	output WR,
	output DC,
	output MIO,
	output LOCK,
	output ADS,
	input NA,
	input READY,		// READY
	input BS16,
	input HOLD,
	output HLDA,
	input BUSY,
	input ERROR,
	input PEREQ,
	input INTR,
	input NMI,
	input RESET,		// RESET
	
	output RESET
);

endmodule